{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "dom.iterable", "es2022"],
    "types": [],
    "downlevelIteration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "useDefineForClassFields": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/app/*": ["./src/app/*"],
      "@/shared/*": ["./src/shared/*"],
      "@/modules/*": ["./src/modules/*"],
      "@/admin/*": ["./src/admin/*"],
      "@/infrastructure/*": ["./src/infrastructure/*"]
    },
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false,
    "noUncheckedSideEffectImports": false
  },
  "include": ["next-env.d.ts", "types/**/*.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": [
    "node_modules",
    ".next",
    "out",
    "dist",
    "build",
    "e2e",
    "examples",
    "scripts",
    "admin-api-backup",
    "backup",
    "**/*.bak",
    "src/lib/core/Logger.ts",
    "src/lib/core/ServiceManager.ts",
    "src/lib/core/BaseService.ts",
    "src/lib/core/Cache.ts",
    "src/lib/core/EventEmitter.ts",
    "src/lib/architecture/**/*",
    "src/app/adscenter/models/ConfigurationStorage.ts",
    "src/app/adscenter/models/EnhancedErrorHandler.ts",
    "src/app/adscenter/models/ErrorManager.ts",
    "src/app/adscenter/models/ExecutionManager.ts",
    "src/app/adscenter/models/ExportService.ts",
    "src/app/adscenter/models/GoogleAdsCampaignUpdater.ts",
    "src/app/adscenter/models/GoogleAdsUpdateExecutor.ts",
    "src/app/adscenter/models/LinkProcessor.ts",
    "src/app/adscenter/models/ResultsStorage.ts",
    "src/app/adscenter/models/SchedulingManager.ts",
    "src/app/adscenter/models/SecureStorage.ts",
    "src/app/adscenter/models/StatusManager.ts",
    "src/app/adscenter/models/StorageManager.ts",
    "src/app/adscenter/models/StorageService.ts",
    "src/app/adscenter/models/UrlExtractionService.ts",
    "src/app/adscenter/models/UrlTrackingService.ts",
    "src/app/adscenter/services/ApiService.ts",
    "src/app/adscenter/tests/OptimizationIntegrationTest.ts",
    "src/app/siterank/SiteRankClientLazy.tsx",
    "src/app/siterank/SiteRankPageRefactored.tsx",
    "src/lib/config/EnvironmentValidator.ts",
    "src/lib/config.ts",
    "src/lib/browser/unified-browser-service.ts",
    "src/lib/cache/EnhancedCache.ts",
    "src/lib/types/consolidated.ts",
    "src/lib/google-ads/unified-google-ads-service.ts",
    "src/lib/legacy-compatibility.ts",
    "**/__tests__/**",
    "**/*.test.*",
    "**/*.spec.*",
    "test-*.cjs",
    "src/lib/services/enhanced-*.ts",
    "src/lib/services/optimized-simplified-proxy-service.ts",
    "src/lib/services/comprehensive-proxy-optimizer.ts",
    "src/lib/services/concurrent-ips1-*.ts",
    "src/lib/services/cycle-proxy-allocation-system.ts",
    "src/lib/services/diversified-request-generator.ts",
    "src/lib/services/header-only-diversified-generator.ts",
    "src/lib/services/proxy-error-recovery-system.ts",
    "src/lib/services/proxy-logging-service.ts",
    "src/lib/services/proxy-performance-benchmark-system.ts",
    "src/lib/services/proxy-pool-monitoring-system.ts",
    "src/lib/services/proxy-quality-assessment-system.ts",
    "src/lib/services/redis-monitor.ts",
    "src/lib/services/redis-unique-constraint-cache.ts",
    "src/lib/services/smart-proxy-calculator.ts",
    "src/lib/services/unique-proxy-allocation-manager.ts",
    "src/lib/services/comprehensive-optimization-test.ts",
    "**/*-old.*"
  ]
}
