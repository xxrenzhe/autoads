<svg xmlns="http://www.w3.org/2000/svg" width="1280" height="800" viewBox="0 0 1280 800">
  <!-- 背景 -->
  <rect width="1280" height="800" fill="#f8fafc"/>
  
  <!-- 浏览器窗口 -->
  <rect x="40" y="40" width="1200" height="720" rx="8" fill="#fff" stroke="#e2e8f0" stroke-width="2"/>
  
  <!-- 浏览器标题栏 -->
  <rect x="40" y="40" width="1200" height="40" rx="8" fill="#f1f5f9"/>
  <circle cx="70" cy="60" r="6" fill="#ef4444"/>
  <circle cx="90" cy="60" r="6" fill="#f59e0b"/>
  <circle cx="110" cy="60" r="6" fill="#10b981"/>
  
  <!-- 地址栏 -->
  <rect x="150" y="50" width="400" height="20" rx="10" fill="#e2e8f0"/>
  <text x="160" y="63" font-size="12" font-family="Arial" fill="#64748b">autoads.dev</text>
  
  <!-- 页面标题 -->
  <text x="640" y="140" text-anchor="middle" font-size="48" font-family="Arial, Helvetica, sans-serif" fill="#1e293b" font-weight="bold">AutoAds</text>
  <text x="640" y="180" text-anchor="middle" font-size="24" font-family="Arial, Helvetica, sans-serif" fill="#64748b">专业URL批量检测与网站排名分析工具</text>
  
  <!-- 功能卡片 -->
  <rect x="200" y="240" width="280" height="200" rx="12" fill="#3b82f6" opacity="0.1"/>
  <text x="340" y="280" text-anchor="middle" font-size="18" font-family="Arial" fill="#1e40af" font-weight="bold">批量打开</text>
  <text x="340" y="320" text-anchor="middle" font-size="14" font-family="Arial" fill="#64748b">高效批量处理URL</text>
  
  <rect x="500" y="240" width="280" height="200" rx="12" fill="#10b981" opacity="0.1"/>
  <text x="640" y="280" text-anchor="middle" font-size="18" font-family="Arial" fill="#059669" font-weight="bold">排名分析</text>
  <text x="640" y="320" text-anchor="middle" font-size="14" font-family="Arial" fill="#64748b">网站权威度评估</text>
  
  <rect x="800" y="240" width="280" height="200" rx="12" fill="#f59e0b" opacity="0.1"/>
  <text x="940" y="280" text-anchor="middle" font-size="18" font-family="Arial" fill="#d97706" font-weight="bold">数据导出</text>
  <text x="940" y="320" text-anchor="middle" font-size="14" font-family="Arial" fill="#64748b">多格式数据导出</text>
  
  <!-- 输入框示意 -->
  <rect x="200" y="500" width="880" height="120" rx="8" fill="#f8fafc" stroke="#e2e8f0" stroke-width="1"/>
  <text x="220" y="530" font-size="14" font-family="Arial" fill="#64748b">输入URL列表...</text>
  
  <!-- 按钮 -->
  <rect x="560" y="650" width="160" height="40" rx="20" fill="#3b82f6"/>
  <text x="640" y="675" text-anchor="middle" font-size="16" font-family="Arial" fill="#fff" font-weight="bold">开始分析</text>
</svg>